extends landing-layout

block content
	.container
		header
			h1.title= title
			h2 Results and benchmarks for TopCoat
		
	.test-navigation
		.container
			h3 Dashboard
			ul#components
				li.category Component
				li#button Button
				li#button_no_theme Button_no_theme
				li#text_input Text Input
				li#input_base Input Base
			ul.button.not-active
				li.category Machine - Button
				li
					a(href='/dashboard?test=loading_benchmark_topcoat_buttons&test=smoothness_benchmark_topcoat_buttons&device=wp-mac01_mac_mini') wp-mac01_mac_mini 
					a(class='button', href='/dashboard?test=loading_benchmark_topcoat_buttons&test=smoothness_benchmark_topcoat_buttons&device=wp-mac01_mac_mini') details
			ul.button_no_theme.not-active
				li.category Machine - Button base
				li
					a(href='/dashboard?test=loading_benchmark_topcoat_buttons_no_theme&test=smoothness_benchmark_topcoat_buttons_no_theme&device=wp-mac01_mac_mini') wp-mac01_mac_mini
					a(class='button', href='/dashboard?test=loading_benchmark_topcoat_buttons_no_theme&test=smoothness_benchmark_topcoat_buttons_no_theme&device=wp-mac01_mac_mini') details
			ul.text_input.not-active
				li.category Machine - Text input
				li
					a(href='/dashboard?test=loading_benchmark_text_input&test=smoothness_benchmark_text_input&device=mbp') mbp 
					a(class='button', href='/dashboard?test=loading_benchmark_text_input&test=smoothness_benchmark_text_input&device=mbp') details
			ul.input_base.not-active
				li.category Machine - Input base
				li
					a(href='/dashboard?test=loading_benchmark_input_base&test=smoothness_benchmark_input_base&device=mbp') mbp
					a(class='button', href='/dashboard?test=loading_benchmark_input_base&test=smoothness_benchmark_input_base&device=mbp') details
			ul.plot
				li.category Plot
				#holder
					.spinner
			h3 View 
				a(href='/view/results') Click for aggregated results

	script(src='/javascripts/raphael-min.js')
	script(src='/javascripts/g.raphael-min.js')
	script(src='/javascripts/g.line-min.js')
	script(src='/javascripts/dashboard.js')
	script(src='/javascripts/landingpage.js')