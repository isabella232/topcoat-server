extends landing-layout

block content
	.container
		header
			.max-width
				hgroup
					h1.title= title
					h4 Results and benchmarks for Topcoat
				nav
					ul
						li: a(href='http://topcoat.io/topcoat') Usage Guidelines
						li: a(href='http://bench.topcoat.io') Benchmarks
						li: a(href='http://topcoat.io/blog') Blog
						li: a.topcoat-button--large--cta(href='http://github.com/topcoat/topcoat/archive/0.5.0.zip') Download v0.5


	.test-navigation
		.container.max-width
			.quarter
				select.docNav
					option(value='/') Dashboard
					option(value='/view/results') Aggregated results
			ul#components.quarter
				li#button.active Button
				li#button_no_theme Button base
				li#text_input Text Input
				li#search_input Search input
				li#radio_input Radio input
				li#range_input Range input
				li#input_base Input Base
				li#textarea Textarea
				li#textarea_base Textarea Base
				li#navigation_bar Navigation bar
				li#navigation_bar_base Navigation bar base
			ul.button.active.three-quarters
				header
					h2 Button
				li
					a(href='/dashboard?test=smoothness_benchmark_topcoat_button&test=loading_benchmark_topcoat_button&device=Nexus4') Nexus 4 
					a.topcoat-button(href='/dashboard?test=smoothness_benchmark_topcoat_button&test=loading_benchmark_topcoat_button&device=Nexus4') details
					a.topcoat-button(href='/baseline?test=smoothness_benchmark_topcoat_button&test=smoothness_benchmark_topcoat_button-base&test=loading_benchmark_topcoat_button&test=smoothness_benchmark_topcoat_button-base&device=Nexus4') baseline
			ul.button_no_theme.not-active.three-quarters
				header
					h2 Button base
				li
					a(href='/dashboard?test=smoothness_benchmark_topcoat_button-base&test=loading_benchmark_topcoat_button-base&device=Nexus4') Nexus 4 
					a.topcoat-button(href='/dashboard?test=smoothness_benchmark_topcoat_button-base&test=loading_benchmark_topcoat_button-base&device=Nexus4') details
			ul.text_input.not-active.three-quarters
				header
					h2 Text input
				li
					a(href='/dashboard?test=loading_benchmark_topcoat_text-input&test=smoothness_benchmark_topcoat_text-input&device=Nexus4') Nexus 4 
					a.topcoat-button(href='/dashboard?test=loading_benchmark_topcoat_text-input&test=smoothness_benchmark_topcoat_text-input&device=Nexus4') details
					a.topcoat-button(href='/baseline?test=loading_benchmark_topcoat_text-input&test=loading_benchmark_topcoat_input-base&test=smoothness_benchmark_topcoat_text-input&test=smoothness_benchmark_topcoat_input-base&device=Nexus4') baseline
			ul.input_base.not-active.three-quarters
