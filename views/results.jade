extends layout

block content
	table
		thead
			th
				select(class='button large')
					option OS
					option Mac
					option Windows
					option iPhone
					option Android
			th
				select(class='button large')
					option Browser
					option Chrome
					option Firefox
					option Safari
			th: .button.large.disabled Version
			th: .button.large.disabled Device
			th: .button.large.disabled Test
			th: .button.large.disabled Result
		tbody
		- results.forEach(function(r){
			tr
				td= r.os
				td= r.browser
				td= r.version
				td= r.device
				td= r.test
				td= r.result
		- })